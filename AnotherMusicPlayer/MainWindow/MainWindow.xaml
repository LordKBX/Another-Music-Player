<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:Collections="clr-namespace:System.Collections;assembly=System.Collections.NonGeneric" 
        xmlns:local="clr-namespace:AnotherMusicPlayer"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        x:Class="AnotherMusicPlayer.MainWindow" Name="win1" mc:Ignorable="d" Title="Another Music Player"
    Height="400" MinHeight="450" Width="550" MinWidth="500">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="D:\CODES\VS\MediaPlayer\AnotherMusicPlayer\Styles\Dark.xaml" />
                <ResourceDictionary Source="D:\CODES\VS\MediaPlayer\AnotherMusicPlayer\Traductions\en.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>

    <Window.TaskbarItemInfo>
        <TaskbarItemInfo x:Name="taskBarItemInfo1" Description="Another Media Player" >
            <TaskbarItemInfo.ThumbButtonInfos>
                <ThumbButtonInfoCollection>
                    <ThumbButtonInfo DismissWhenClicked="False" x:Name="PreviewCtrlPrev" Description="PreviousTrack" RenderOptions.BitmapScalingMode="HighQuality" />
                    <ThumbButtonInfo DismissWhenClicked="False" x:Name="PreviewCtrlPause" Description="Play" RenderOptions.BitmapScalingMode="HighQuality" />
                    <ThumbButtonInfo DismissWhenClicked="False" x:Name="PreviewCtrlNext" Description="NextTrack" RenderOptions.BitmapScalingMode="HighQuality" />
                </ThumbButtonInfoCollection>
            </TaskbarItemInfo.ThumbButtonInfos>
        </TaskbarItemInfo>
    </Window.TaskbarItemInfo>

    <materialDesign:DialogHost CloseOnClickAway="False" x:Name="dialog1" IsOpen="False">
        <materialDesign:DialogHost.DialogContent>
            <Grid Margin="20" MinWidth="200px" Height="50px" MaxHeight="50px">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="50px"/>
                    <ColumnDefinition Width="100*"/>
                </Grid.ColumnDefinitions>
                <Image x:Name="dialog1Image" Source="/AnotherMusicPlayer;component/icons/loadingx50.png" Grid.Column="0" MaxHeight="50px" MaxWidth="50px" Height="35px" Width="35px">
                    <Image.Style>
                        <Style TargetType="{x:Type Image}">
                            <Setter Property="LayoutTransform">
                                <Setter.Value>
                                    <RotateTransform Angle="0"/>
                                </Setter.Value>
                            </Setter>
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding InLoading}" Value="Running">
                                    <DataTrigger.EnterActions>
                                        <BeginStoryboard>
                                            <Storyboard>
                                                <DoubleAnimation Storyboard.TargetProperty="(LayoutTransform).(RotateTransform.Angle)"
                                                     From="0"
                                                     To="360"
                                                     Duration="0:0:2"
                                                     RepeatBehavior="Forever"/>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </DataTrigger.EnterActions>
                                    <DataTrigger.ExitActions>
                                        <BeginStoryboard>
                                            <Storyboard FillBehavior="Stop">
                                                <DoubleAnimation Storyboard.TargetProperty="(LayoutTransform).(RotateTransform.Angle)"
                                                     To="90"
                                                     Duration="0:0:0" />
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </DataTrigger.ExitActions>

                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Image.Style>
                </Image>
                <TextBlock x:Name="dialog1Text" Text="Loading" Grid.Column="1" FontSize="22" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </Grid>
        </materialDesign:DialogHost.DialogContent>

        <Grid Style="{DynamicResource CustomWindowBackgroundStyle}">
            <Grid.RowDefinitions>
                <RowDefinition Height="100*"/>
                <RowDefinition Height="40px"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="100*"/>
                <ColumnDefinition Width="50px"/>
            </Grid.ColumnDefinitions>

            <TabControl Grid.Row="0" Grid.Column="0" x:Name="TabControler" BorderBrush="{x:Null}" Background="{x:Null}" 
                         Margin="0" Padding="0" materialDesign:NavigationRailAssist.ShowSelectionBackground="True" 
                         SnapsToDevicePixels="True" Style="{StaticResource TabControlStyle}"
                        TabStripPlacement="Top">

                <TabItem Header="{DynamicResource TabPlaylist}" x:Name="Tab1" Style="{DynamicResource TabItemStyle1}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                    <Grid x:Name="Grid1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="150px"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="100*"/>
                        </Grid.RowDefinitions>

                        <Border Grid.Row="0" Grid.Column="0" BorderBrush="Gray" BorderThickness="0,0,1,0" Margin="0,0,0,0">
                            <Grid x:Name="Grid2" Background="#FF666666" Panel.ZIndex="99999">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="23*"/>
                                    <ColumnDefinition Width="52*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="150px"/>
                                    <RowDefinition Height="100*"/>
                                </Grid.RowDefinitions>

                                <Border BorderThickness="0,1,0,1" BorderBrush="LightGray" Grid.ColumnSpan="2">
                                    <TextBlock Background="Black">
                                <Image x:Name="FileCover" Grid.Row="0" Width="150" Height="150" RenderOptions.BitmapScalingMode="HighQuality"/>
                                    </TextBlock>
                                </Border>
                                <Border BorderThickness="0,1,0,1" BorderBrush="Gray" Grid.Row="1" Grid.ColumnSpan="2">
                                    <ScrollViewer ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.CanContentScroll="True" Background="#FF303030">
                                        <TextBlock x:Name="LeftPannelMediaInfo" Foreground="White">
                                    <TextBlock Text="{DynamicResource Title2}" x:Name="PlayItemNameTitle" Margin="3,3,0,0"/> <LineBreak></LineBreak>
                                    <AccessText x:Name="PlayItemNameValue" Margin="10,0,0,0" Foreground="#FF959595" TextWrapping="WrapWithOverflow" Text=""/>
                                    <LineBreak></LineBreak>
                                    <TextBlock Text="{DynamicResource Album2}" x:Name="PlayItemAlbumTitle" Margin="3,0,0,0"/> <LineBreak></LineBreak>
                                    <AccessText   x:Name="PlayItemAlbumValue" Text="" Margin="10,0,0,0" Foreground="#FF959595" TextWrapping="WrapWithOverflow"/>
                                    <LineBreak></LineBreak>
                                    <TextBlock Text="{DynamicResource Artist2}" x:Name="PlayItemArtistsTitle" Margin="3,0,0,0"/> <LineBreak></LineBreak>
                                    <AccessText   x:Name="PlayItemArtistsValue" Text="" Margin="10,0,0,0" Foreground="#FF959595" TextWrapping="WrapWithOverflow"/>
                                    <LineBreak></LineBreak>
                                    <TextBlock Visibility="Collapsed" Text="{DynamicResource duration2}" x:Name="PlayItemdurationTitle" Margin="3,0,0,0"/> <LineBreak></LineBreak>
                                    <TextBlock Visibility="Collapsed"  x:Name="PlayItemDurationValue" Text="" Margin="10,0,0,0" Foreground="#FF959595"/>
                                </TextBlock>
                                    </ScrollViewer>
                                </Border>
                            </Grid>
                        </Border>
                        <Grid x:Name="Grid3" Grid.Row="0" Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="9*"/>
                                <ColumnDefinition Width="112*"/>
                                <ColumnDefinition Width="121*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="20px"/>
                                <RowDefinition Height="100*"/>
                            </Grid.RowDefinitions>
                            <Border Grid.Row="0" BorderBrush="LightGray" BorderThickness="1,1,1,0" Margin="0,0,1,0" Grid.ColumnSpan="3">
                                <TextBlock Foreground="White" Padding="5,0,0,0" VerticalAlignment="Stretch" HorizontalAlignment="Left">
                                    <TextBlock>Pistes à venir:</TextBlock><Run Text=" "/>
                                    <TextBlock x:Name="Label_PlayListDisplayedNBTracks" /><TextBlock>, Total:</TextBlock><Run Text=" "/>
                                    <TextBlock x:Name="Label_PlayListNBTracks" /><TextBlock>, Position:</TextBlock>
                                    <TextBlock x:Name="Label_PlayListIndex" />
                                </TextBlock>
                            </Border>
                            <ListView x:Name="PlayListView" Grid.Row="1" SelectionMode="Extended" Background="Gray" IsTextSearchEnabled="False" AutomationProperties.IsColumnHeader="True" ScrollViewer.HorizontalScrollBarVisibility="Hidden" ListView.ItemContainerStyle="{DynamicResource ResourceKey=MediaItem}" AlternationCount="2" HorizontalContentAlignment="Left" BorderBrush="LightGray" BorderThickness="1,0,1,1" Padding="-4,-1,0,0" VerticalContentAlignment="Center" FlowDirection="LeftToRight" Margin="0,0,1,0" Grid.ColumnSpan="3">
                                <ListView.View>
                                    <GridView AllowsColumnReorder="False">
                                        <GridViewColumn x:Name="PlayListViewC1" Width="25" Header="" DisplayMemberBinding="{Binding Selected}" HeaderContainerStyle="{DynamicResource ListViewHeaderStyle}"/>
                                        <GridViewColumn x:Name="PlayListViewC2" Header="{DynamicResource Title}" HeaderContainerStyle="{DynamicResource ListViewHeaderStyle}">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <AccessText Text="{Binding Name}" Style="{DynamicResource ListViewColumnItemAccessText}"/>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn x:Name="PlayListViewC5" Width="80" Header="{DynamicResource Duration}" DisplayMemberBinding="{Binding DurationS}" HeaderContainerStyle="{DynamicResource ListViewHeaderStyle}"/>
                                        <GridViewColumn x:Name="PlayListViewC4" Header="{DynamicResource Artist}" HeaderContainerStyle="{DynamicResource ListViewHeaderStyle}">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <AccessText Text="{Binding Performers}" Style="{DynamicResource ListViewColumnItemAccessText}"/>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn x:Name="PlayListViewC3" Header="{DynamicResource Album}" HeaderContainerStyle="{DynamicResource ListViewHeaderStyle}">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <AccessText Text="{Binding Album}" Style="{DynamicResource ListViewColumnItemAccessText}" Margin="0,0,20,0"/>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                    </GridView>
                                </ListView.View>
                            </ListView>
                        </Grid>
                    </Grid>
                </TabItem>
                <TabItem Header="{DynamicResource TabLibrary}" Style="{DynamicResource TabItemStyle2}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Cursor="Arrow">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="25"/>
                            <RowDefinition Height="25"/>
                            <RowDefinition Height="100*"/>
                        </Grid.RowDefinitions>

                        <Border BorderThickness="0,1,0,1" BorderBrush="#FFACACAC">
                            <Viewbox HorizontalAlignment="Left" VerticalAlignment="Stretch" FlowDirection="LeftToRight">
                                <WrapPanel x:Name="LibibraryNavigationPathContener" Style="{DynamicResource LibibraryNavigationPathContener}">
                                    <TextBlock Style="{DynamicResource LibibraryNavigationPathItem}" TextDecorations="Underline" Text="Home"/>
                                </WrapPanel>
                            </Viewbox>
                        </Border>
                        <Grid Grid.Row="1" x:Name="LibraryFiltersGrid">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="13" />
                                <ColumnDefinition Width="57"/>
                                <ColumnDefinition Width="100" />
                                <ColumnDefinition Width="100" />
                                <ColumnDefinition Width="100" />
                                <ColumnDefinition Width="100*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock Text="Filtre" VerticalAlignment="Center" Margin="5,0,0,0" Grid.ColumnSpan="2"/>
                            <ComboBox Grid.Column="2" x:Name="LibraryFiltersMode" Style="{DynamicResource LibibraryNavigationComboBox}">
                                <ComboBoxItem x:Name="LibraryFiltersMode_none" Tag="" Content="Aucun" Selector.IsSelected="True"/>
                                <ComboBoxItem x:Name="LibraryFiltersMode_name" Tag="Name" Content="Nom"/>
                                <ComboBoxItem x:Name="LibraryFiltersMode_artist" Tag="Artist" Content="Artist"/>
                                <ComboBoxItem x:Name="LibraryFiltersMode_album" Tag="Album" Content="Album"/>
                                <ComboBoxItem x:Name="LibraryFiltersMode_genre" Tag="Genre" Content="Genre"/>
                            </ComboBox>
                            <ComboBox Grid.Column="3" x:Name="LibraryFiltersGenreList" Visibility="Hidden" Style="{DynamicResource LibibraryNavigationComboBox}"></ComboBox>
                            <TextBox Grid.Column="4" x:Name="LibraryFiltersGenreSearchBox" Visibility="Hidden" Cursor="IBeam"/>
                            <TextBox Grid.Column="3" Grid.ColumnSpan="2" x:Name="LibraryFiltersSearchBox" Style="{DynamicResource LibibraryNavigationFilterInput}" Visibility="Collapsed"/>
                            <Grid Grid.Column="5" x:Name="LibraryFiltersPaginationBlock" HorizontalAlignment="Left" Visibility="Hidden" Margin="38,0,0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition Width="50" />
                                    <ColumnDefinition Width="20" />
                                </Grid.ColumnDefinitions>
                                <Button Grid.Column="0" x:Name="LibraryFiltersPaginationPrevious" FontFamily="Segoe UI Symbol" Content="◀"/>
                                <TextBlock Grid.Column="1" x:Name="LibraryFiltersPaginationDisplay" Text="1/x" TextAlignment="Center" VerticalAlignment="Center"/>
                                <Button Grid.Column="2" x:Name="LibraryFiltersPaginationNext" FontFamily="Segoe UI Symbol" Content="▶"/>
                            </Grid>
                        </Grid>
                        <Border x:Name="LibibraryNavigationContentB" Grid.Row="2" BorderThickness="0,1,0,0" BorderBrush="#FFACACAC" Visibility="Visible">
                            <ScrollViewer x:Name="LibibraryNavigationContentScroll" VerticalScrollBarVisibility="Auto" Foreground="Black">
                                <WrapPanel x:Name="LibibraryNavigationContent"></WrapPanel>
                            </ScrollViewer>
                        </Border>
                        <Border x:Name="LibibraryNavigationContent2B" Grid.Row="2" BorderThickness="0,1,0,0" BorderBrush="Black" Visibility="Collapsed">
                            <ScrollViewer x:Name="LibibraryNavigationContentScroll2" VerticalScrollBarVisibility="Auto" Foreground="Black">
                                <StackPanel x:Name="LibibraryNavigationContent2">
                                </StackPanel>
                            </ScrollViewer>
                        </Border>
                        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="2" Margin="36,13,0,0" VerticalAlignment="Top" Width="63"/>
                    </Grid>
                </TabItem>
                <TabItem Header="{DynamicResource TabRecordedPlaylists}" Style="{DynamicResource TabItemStyle3}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                    <Grid Margin="-2,0,4,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="150" />
                            <ColumnDefinition Width="100*" />
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0" Margin="1,0,1,2">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="100*"/>
                            </Grid.RowDefinitions>
                            
                            <Border Grid.Row="0" Background="SlateGray" Margin="1,2,0,2" BorderThickness="0">
                                <TextBlock Text="{DynamicResource PlayListsHeaderCol1}" Foreground="White" VerticalAlignment="Center" Height="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                            </Border>
                            <TreeView x:Name="PlaylistsTree"  Grid.Row="1" Margin="1,0,0,0" 
                                      Style="{DynamicResource PlaylistsTreeStyle}"
                                      ScrollViewer.VerticalScrollBarVisibility="Disabled" 
                                      ScrollViewer.CanContentScroll="False" ContextMenuService.IsEnabled="True">
                                <TreeViewItem x:Name="PlaylistsTreeAuto" Header="Automatique" Style="{DynamicResource PlaylistsTreeStyleItem}">
                                    <TreeViewItem Header="BB" Style="{DynamicResource PlaylistsTreeStyleItem}"></TreeViewItem>
                                </TreeViewItem>
                                <TreeViewItem Header="CC" Style="{DynamicResource PlaylistsTreeStyleItem}">
                                </TreeViewItem>
                            </TreeView>
                        </Grid>
                        <Grid Grid.Column="1" Margin="1,0,2,2">
                            <ListView x:Name="PlaylistsContents" Grid.Row="1" SelectionMode="Extended" 
                                      Background="Gray" IsTextSearchEnabled="False" 
                                      AutomationProperties.IsColumnHeader="True" 
                                      ScrollViewer.HorizontalScrollBarVisibility="Hidden" 
                                      ListView.ItemContainerStyle="{DynamicResource ResourceKey=MediaItem}" 
                                      AlternationCount="2" HorizontalContentAlignment="Left" 
                                      BorderBrush="LightGray" BorderThickness="1,0,1,1" Padding="-4,-1,0,0" 
                                      VerticalContentAlignment="Center" FlowDirection="LeftToRight" 
                                      Margin="0,0,1,0" Grid.ColumnSpan="3">
                                <ListView.View>
                                    <GridView AllowsColumnReorder="False">
                                        <GridViewColumn x:Name="PlaylistsContentsC0" Header="{DynamicResource Count}" 
                                                        HeaderContainerStyle="{DynamicResource ListViewHeaderStyle}">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <AccessText Text="{Binding PlayCount}" 
                                                                Style="{DynamicResource ListViewColumnItemAccessText}"/>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn x:Name="PlaylistsContentsC1" Header="{DynamicResource Title}" 
                                                        HeaderContainerStyle="{DynamicResource ListViewHeaderStyle}">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <AccessText Text="{Binding Name}" 
                                                                Style="{DynamicResource ListViewColumnItemAccessText}"/>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn x:Name="PlaylistsContentsC2" 
                                                        Header="{DynamicResource Artist}" 
                                                        HeaderContainerStyle="{DynamicResource ListViewHeaderStyle}">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <AccessText Text="{Binding Performers}" 
                                                                Style="{DynamicResource ListViewColumnItemAccessText}"/>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn x:Name="PlaylistsContentsC3" 
                                                        Header="{DynamicResource Album}" 
                                                        HeaderContainerStyle="{DynamicResource ListViewHeaderStyle}">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <AccessText Text="{Binding Album}" 
                                                                Style="{DynamicResource ListViewColumnItemAccessText}" 
                                                                Margin="0,0,20,0"/>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn x:Name="PlaylistsContentsC4" Width="80" 
                                                        Header="{DynamicResource Duration}" 
                                                        DisplayMemberBinding="{Binding DurationS}" 
                                                        HeaderContainerStyle="{DynamicResource ListViewHeaderStyle}"/>
                                        <GridViewColumn x:Name="PlaylistsContentsC5" 
                                                        Header="{DynamicResource Rating}" Width="130"
                                                        HeaderContainerStyle="{DynamicResource ListViewHeaderStyle}">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <local:Rating Zoom="0.7" StarBackgroundColor="Gray" 
                                                        StarForegroundColor="White" Rate="{Binding Rating}" 
                                                        RateChanged="RatingRateChanged" Tag="{Binding Path}" 
                                                                  IsReadOnly="True"/>
                                                    <!--<materialDesign:RatingBar 
                                                        Value="{Binding Rating}" 
                                                        IsReadOnly="True" Foreground="White" Opacity="1"/>-->
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                    </GridView>
                                </ListView.View>
                            </ListView>
                        </Grid>
                    </Grid>
                    <!--
            <UniformGrid Background="#FFCFCFCF" Margin="0,0,4,0">
                <AccessText Text="  Comming Soon !  " Foreground="Black" Background="#FFCFCFCF" Margin="0,0,4,0" TextAlignment="Center" FontSize="48" VerticalAlignment="Center" />
            </UniformGrid>
            -->
                </TabItem>
                <TabItem Header="{DynamicResource TabParams}" Style="{DynamicResource TabItemStyle4}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Cursor="Arrow">
                    <Border Grid.Row="0" Grid.Column="0" Style="{DynamicResource TabItemStyleSettings}">
                        <ScrollViewer VerticalScrollBarVisibility="Auto">
                            <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100px"/>
                                    <ColumnDefinition Width="200px"/>
                                    <ColumnDefinition Width="100*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="5px"/>
                                    <RowDefinition Height="30px"/>
                                    <RowDefinition Height="10px"/>
                                    <RowDefinition Height="30px"/>
                                    <RowDefinition Height="10px"/>
                                    <RowDefinition Height="20px"/>
                                    <RowDefinition Height="25px"/>
                                    <RowDefinition Height="25px"/>
                                    <RowDefinition Height="10px"/>
                                    <RowDefinition Height="25px"/>
                                    <RowDefinition Height="10px"/>
                                    <RowDefinition Height="20px"/>
                                    <RowDefinition Height="20px"/>
                                    <RowDefinition Height="10px"/>
                                    <RowDefinition Height="30px"/>
                                    <RowDefinition Height="5px"/>
                                    <RowDefinition Height="150px"/>
                                    <RowDefinition Height="100*"/>
                                </Grid.RowDefinitions>
                                <Label Content="{DynamicResource ParamsLanguageLabel}" Style="{DynamicResource TabItemStyleSettingsText}" Grid.Column="0" Grid.Row="1" VerticalAlignment="Center"/>
                                <ComboBox x:Name="ParamsLanguageVals" Style="{DynamicResource TabItemStyleSettingsComboBox}" Grid.Column="1" Grid.Row="1" MinWidth="100" HorizontalAlignment="Stretch" VerticalAlignment="Center">
                                    <ComboBoxItem x:Name="LG_en" Tag="en-US" Content="{DynamicResource ParamsLanguageVals_EN}" IsSelected="False"/>
                                    <ComboBoxItem x:Name="LG_fr" Tag="fr-FR" Content="{DynamicResource ParamsLanguageVals_FR}" IsSelected="True"/>
                                </ComboBox>

                                <Label Content="{DynamicResource ParamsStyleLabel}" Style="{DynamicResource TabItemStyleSettingsText}" Grid.Column="0" Grid.Row="3" VerticalAlignment="Center"/>
                                <ComboBox x:Name="ParamsStyleVals" Style="{DynamicResource TabItemStyleSettingsComboBox}" Grid.Column="1" Grid.Row="3" MinWidth="100" HorizontalAlignment="Stretch" VerticalAlignment="Center">
                                </ComboBox>

                                <AccessText Text="{DynamicResource ParamsConvDisclaimerLabel}" Style="{DynamicResource TabItemStyleSettingsText2}" Grid.Column="0" Grid.Row="5" Grid.ColumnSpan="3" VerticalAlignment="Center" TextWrapping="WrapWithOverflow" Margin="10,0,0,0"/>
                                <Label Content="{DynamicResource ParamsConvKeepLabel}" Style="{DynamicResource TabItemStyleSettingsText}" Grid.Column="0" Grid.Row="6" VerticalAlignment="Center"/>
                                <ComboBox x:Name="ParamsConvKeepVals" Style="{DynamicResource TabItemStyleSettingsComboBox}" Grid.Column="1" Grid.Row="6" MinWidth="100" HorizontalAlignment="Stretch" VerticalAlignment="Center">
                                    <ComboBoxItem x:Name="CT1" Tag="1" Content="{DynamicResource ParamsConvKeepVals_TEMP}" IsSelected="True"/>
                                    <ComboBoxItem x:Name="CT2" Tag="2" Content="{DynamicResource ParamsConvKeepVals_REPLACE}" IsSelected="False"/>
                                </ComboBox>
                                <Label Content="{DynamicResource ParamsConvQualityLabel}" Style="{DynamicResource TabItemStyleSettingsText}" Grid.Column="0" Grid.Row="7" VerticalAlignment="Center"/>
                                <ComboBox x:Name="ParamsConvQualityVals" Style="{DynamicResource TabItemStyleSettingsComboBox}" Grid.Column="1" Grid.Row="7" MinWidth="100" HorizontalAlignment="Stretch" VerticalAlignment="Center">
                                    <ComboBoxItem x:Name="Q96" Tag="96" Content="{DynamicResource ParamsConvQualityVals_96}" IsSelected="False"/>
                                    <ComboBoxItem x:Name="Q128" Tag="128" Content="{DynamicResource ParamsConvQualityVals_128}" IsSelected="False"/>
                                    <ComboBoxItem x:Name="Q192" Tag="192" Content="{DynamicResource ParamsConvQualityVals_192}" IsSelected="False"/>
                                    <ComboBoxItem x:Name="Q256" Tag="256" Content="{DynamicResource ParamsConvQualityVals_256}" IsSelected="False"/>
                                    <ComboBoxItem x:Name="Q320" Tag="320" Content="{DynamicResource ParamsConvQualityVals_320}" IsSelected="False"/>
                                </ComboBox>
                                <Label Content="{DynamicResource ParamsMemoryUsageLabel}" Style="{DynamicResource TabItemStyleSettingsText}" Grid.Column="0" Grid.Row="9" VerticalAlignment="Center"/>
                                <ComboBox x:Name="ParamsMemoryUsage" Style="{DynamicResource TabItemStyleSettingsComboBox}" Grid.Column="1" Grid.Row="9" MinWidth="100" HorizontalAlignment="Stretch" VerticalAlignment="Center">
                                    <ComboBoxItem x:Name="MU0" Tag="0" Content="{DynamicResource ParamsMemoryUsage0}" IsSelected="False"/>
                                    <ComboBoxItem x:Name="MU1" Tag="1" Content="{DynamicResource ParamsMemoryUsage1}" IsSelected="True"/>
                                </ComboBox>

                                <AccessText Text="{DynamicResource ParamsLibFolderLabel}" Style="{DynamicResource TabItemStyleSettingsText2}" Grid.Column="0" Grid.Row="11" Grid.ColumnSpan="3" VerticalAlignment="Center" TextWrapping="WrapWithOverflow" Margin="12,0,0,0"/>
                                <TextBox x:Name="ParamsLibFolderTextBox" Style="{DynamicResource TabItemStyleSettingsTextBox}" Grid.Column="0" Grid.Row="12" Grid.ColumnSpan="2" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" VerticalContentAlignment="Center" Margin="10,0,0,0" Padding="0"/>
                                <Button x:Name="ParamsLibFolderBtn" Style="{DynamicResource TabItemStyleSettingsForLibraryFolderButton}" Grid.Column="2" Grid.Row="12" Content=" ... " HorizontalAlignment="Left" Width="30" Click="ParamsLibFolderBtn_Click" Padding="0,0,0,0" VerticalAlignment="Stretch"/>


                                <AccessText Text="{DynamicResource ParamsEqualizerLabel}" Style="{DynamicResource TabItemStyleSettingsText2}" Grid.Column="0" Grid.Row="14" VerticalAlignment="Center" TextWrapping="WrapWithOverflow" Margin="12,0,0,0"/>
                                <ComboBox  x:Name="ParamsEqualizerPresets" Style="{DynamicResource TabItemStyleSettingsComboBox}" Grid.Column="1" Grid.Row="14" MinWidth="100" HorizontalAlignment="Stretch" VerticalAlignment="Center">
                                    <ComboBoxItem x:Name="EQ_perso" Tag="Perso" Content="{DynamicResource ParamsEqualizerPresets_Perso}" IsSelected="True"/>
                                    <ComboBoxItem x:Name="EQ_flat" Tag="Flat" Content="{DynamicResource ParamsEqualizerPresets_Flat}" IsSelected="False"/>
                                    <ComboBoxItem x:Name="EQ_classic" Tag="Classic" Content="{DynamicResource ParamsEqualizerPresets_Classic}" IsSelected="False"/>
                                    <ComboBoxItem x:Name="EQ_club" Tag="Club" Content="{DynamicResource ParamsEqualizerPresets_Club}" IsSelected="False"/>
                                    <ComboBoxItem x:Name="EQ_dance" Tag="Dance" Content="{DynamicResource ParamsEqualizerPresets_Dance}" IsSelected="False"/>
                                    <ComboBoxItem x:Name="EQ_fullbass" Tag="FullBass" Content="{DynamicResource ParamsEqualizerPresets_FullBass}" IsSelected="False"/>
                                    <ComboBoxItem x:Name="EQ_fullbasstreble" Tag="FullBassTreble" Content="{DynamicResource ParamsEqualizerPresets_FullBassTreble}" IsSelected="False"/>
                                    <ComboBoxItem x:Name="EQ_fulltreble" Tag="FullTreble" Content="{DynamicResource ParamsEqualizerPresets_FullTreble}" IsSelected="False"/>
                                    <ComboBoxItem x:Name="EQ_headphones" Tag="Headphones" Content="{DynamicResource ParamsEqualizerPresets_Headphones}" IsSelected="False"/>
